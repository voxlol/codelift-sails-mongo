  "services": [
    {
      "name": "app",
      "image": "",
      "target_num_containers": 1,
      "sequential_deployment": true,
      "autorestart": "ALWAYS",
      "autoredeploy": true,
      "linked_to_service": [
        {
          "name": "db",
          "to_service": "db"
        }
      ],
      "container_envvars": [
        {
          "key": "DATABASE_URL",
          "value": "mongodb://:O-LjFRe3CuYNnktsosCvyw@db:27017/"
        },
        {
          "key": "NODE_ENV",
          "value": "production"
        }
      ],
      "container_ports": [
        {
          "protocol": "tcp",
          "inner_port": 1337,
          "published": true,
          "outer_port": 7394
